{
  "name": "Trimp-desktop",
  "version": "1.1.0",
  "description": "Trimp Desktop",
  "repository": {
    "type": "git",
    "url": "https://github.com/scolpitts536/Trimp-desktop.git"
  },
  "main": "src/main.js",
  "scripts": {
    "start": "electron ./",
    "restore-src": "git restore --source=HEAD --staged --worktree -- src/Trimps-master && git clean -x -f -- src/Trimps-master",
    "dist:32": "electron-builder --ia32",
    "dist:64": "electron-builder --x64 -c.artifactName=${productName}-64bit-setup-${version}.${ext}",
    "win": "electron-builder --windows portable",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": {
    "name": "MrPitimp"
  },
  "license": "ISC",
  "dependencies": {
    "adm-zip": "^0.5.5",
    "discord-rpc": "^4.0.1",
    "electron-updater": "^4.3.9"
  },
  "devDependencies": {
    "electron": "^13.1.5",
    "electron-builder": "^22.11.7"
  },
  "build": {
    "productName": "Trimps",
    "icon": "/build/trimps.png",
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "win": {
      "requestedExecutionLevel": "highestAvailable"
    },
    "linux": {
      "category": "Game",
      "icon": "icon_512x512.png"
    },
    "deb": {},
    "appId": "Trimps.desktop"
  }
}